static inline int square(int arg) {
    return arg * arg;
}

/* Oneline traditional comment. */

static inline int cube(int arg) {
    return arg * arg * arg;
}

// Oneline linear comment.

static inline int doubling(int arg) {
    return 2 * arg;
}
